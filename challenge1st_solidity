pragma solidity 0.8.9;
contract mycontract {
    string mystring;
    uint myint;
    bool mybool;
    address myaddress;
    
    function setmystring(string memory minestring) public {
        mystring=minestring;
    }
    function getmystring() public view returns (string memory){
        return mystring;
    }

    function setmyint(uint mineint) public {
        myint=mineint;
    }
    function getmyint() public view returns (uint){
        return myint;
    }
    function setmybool(bool minebool) public {
        mybool=minebool;
    }
    function getmybool() public view returns (bool){
        return mybool;
    }
    function setmyaddress(address mineaddress) public {
        myaddress=mineaddress;
    }
    function getmyaddress() public view returns (address){
        return myaddress;
    }
}
